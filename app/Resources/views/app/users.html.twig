{% extends '::base.html.twig' %}

{% block body %}
    <h3>Users with their hobbies listed:</h3>
    <ul>
    {% for user in users %}
        <li>
            {{ user.username }}
            <ul>
                {% for hobby in user.hobbies %}
                    <li>{{ hobby.name }}</li>
                {% endfor %}
            </ul>
        </li>
    {% endfor %}
    </ul>
{% endblock %}